{
  "name": "jessica-janiuk-api",
  "version": "0.0.1",
  "description": "Jessica Janiuk's personal website API",
  "main": "dist",
  "env": "development",
  "repository": "https://github.com/janiukjf/JessicaJaniukApi",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "./node_modules/.bin/babel src -s -D -d dist --presets es2015,stage-0",
    "migrate": "./node_modules/.bin/sequelize db:migrate --config dist/config/config.json --migrations-path dist/migrations",
    "start": "npm run -s build && npm run migrate && node dist",
    "test": "mocha 'src/**/*.spec.js' && eslint . --ext .js"
  },
  "author": "Jessica Janiuk",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "express": "^4.14.0",
    "express-jwt-token": "^0.1.5",
    "jsonwebtoken": "^7.2.1",
    "pg": "^6.1.2",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^3.29.0",
    "sequelize-cli": "^2.5.1",
    "sequelize-heroku": "^1.0.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.7"
  }
}
